{
    "manifest_version": 3,
    "name": "Tab Title Extension",
    "version": "1.0",
    "description": "Displays the current tab title when the button is clicked.",
    "permissions": [
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  